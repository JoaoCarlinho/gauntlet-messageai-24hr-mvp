{
  "name": "mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:notifications": "jest --testPathPatterns=notification",
    "test:notifications:unit": "jest --testPathPatterns=notification.service.test.ts",
    "test:notifications:handler": "jest --testPathPatterns=message.handler.test.ts",
    "test:notifications:mobile": "jest --testPathPatterns=useNotifications.test.ts",
    "test:notifications:e2e": "jest --testPathPatterns=foregroundNotification.test.ts",
    "test:notifications:all": "npm run test:notifications:unit && npm run test:notifications:handler && npm run test:notifications:mobile && npm run test:notifications:e2e"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "2.2.0",
    "@tanstack/react-query": "^5.90.5",
    "axios": "^1.12.2",
    "expo": "~54.0.0",
    "expo-constants": "~18.0.10",
    "expo-device": "^8.0.9",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.0",
    "expo-notifications": "^0.32.12",
    "expo-router": "~6.0.0",
    "expo-secure-store": "~15.0.7",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "kea": "^3.1.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/node-fetch": "^2.6.13",
    "@types/react": "~19.1.0",
    "@types/react-native": "^0.72.8",
    "jest": "~29.7.0",
    "jest-expo": "^54.0.13",
    "node-fetch": "^2.7.0",
    "react-test-renderer": "^19.1.0",
    "typescript": "~5.9.2"
  },
